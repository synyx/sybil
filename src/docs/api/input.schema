{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "type": {
      "type": "string",
      "enum": ["status", "binary"]
    },
    "source": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 2,
      "maxItems": 2
    },
    "state": {
      "type": "string",
      "enum": [ true, false, "OK", "WARNING", "CRITICAL"]
    }
  },
  "required": [ "type", "source" ]
}